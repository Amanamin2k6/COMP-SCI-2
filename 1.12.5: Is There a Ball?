// Your code should work correctly in each world

// Karel should put a ball on the first spot
// if there isn't one already there and then move.
function start() {
    if (ballsPresent()){
	    hasBallTrue();
    } else {
	    hasBallFalse();
    }
}
function hasBallTrue(){
    move();
}
function hasBallFalse(){
    putBall();
    move();
}
