function start(){
	var line = ["Sam", "Lisa", "Laurie", "Bob", "Ryan"];
//print the original
    printline(line);
//remove the first 2 elements
    line.remove(0);
    line.remove(0);
//print the modified
    printline(line);
    
}
function printline(line){
//take every element in the lin list and print it
    for (var i = 0; i < line.length; i++){
//print every element followed by a comma except the last one
        if (i < (line.length-1)){
            print(line[i] + ", ")
        } else {
            print(line[i]);
//start a new line
            println("");
        }
    }
}
